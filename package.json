{
  "name": "money-me-app-2025",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "eslint src/**/*.ts src/**/*.html",
    "lint:fix": "eslint src/**/*.ts src/**/*.html --fix",
    "backend": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "dev": "concurrently \"npm run start:all\" \"npm run backend\"",
    "dev:build": "concurrently \"npm run build\" \"npm run backend\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "start:all": "powershell -ExecutionPolicy Bypass -File start-app-reliable.ps1",
    "start:local": "powershell -ExecutionPolicy Bypass -File start-app-local.ps1",
    "stop:all": "powershell -ExecutionPolicy Bypass -File stop-app.ps1",
    "stop:local": "powershell -ExecutionPolicy Bypass -File stop-app-local.ps1",
    "restart:all": "powershell -ExecutionPolicy Bypass -File restart-app.ps1",
    "setup:local": "powershell -ExecutionPolicy Bypass -File setup-local-postgres.ps1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.5",
    "@angular/common": "^20.1.5",
    "@angular/compiler": "^20.1.6",
    "@angular/core": "^20.1.5",
    "@angular/forms": "^20.1.5",
    "@angular/localize": "^20.1.6",
    "@angular/platform-browser": "^20.1.5",
    "@angular/platform-browser-dynamic": "^20.1.5",
    "@angular/platform-server": "^20.1.5",
    "@angular/router": "^20.1.5",
    "@angular/ssr": "^20.1.5",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@popperjs/core": "^2.11.8",
    "@types/chart.js": "^2.9.41",
    "bootstrap": "^5.3.7",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.6",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "angular-eslint": "20.1.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.32.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.34.1",
    "wait-on": "^8.0.4"
  }
}
